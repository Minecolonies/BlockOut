{
  "id": "template_blockstate_selector",
  "type": "blockout:template",
  "children": [
    {
      "id": "selector_button",
      "type": "blockout:button",
      "size": "26,26",
      "margin": "2,2",
      "defaultBackgroundImage": "image:button_normal",
      "disabledBackgroundImage": "image:button_disabled",
      "clickedBackgroundImage": "image:button_clicked",
      "children": [
        {
          "id": "selector_contents",
          "type": "blockout:blockStateIcon",
          "size": "16,16",
          "alignment": "LEFT,RIGHT,TOP,BOTTOM",
          "blockState": "{Binding:blockState}"
        }
      ]
    }
  ]
}